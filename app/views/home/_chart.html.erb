<script>
$(function () {
    $('#container').highcharts({
        chart: {
            type: 'line'
        },
        title: {
            text: 'Case wise Business Analysis'
        },
        subtitle: {
            text: ''
        },
        xAxis: {
            categories: <%= raw @line_chart_data.keys.map {|datetime| datetime.strftime("%d %b")} %>
        },
        yAxis: {
            title: {
                text: 'Number of cases'
            }
        },
        plotOptions: {
            line: {
                dataLabels: {
                    enabled: true
                },
                enableMouseTracking: false
            }
        },
        series: [{
            name: 'Case Count',
            data: <%= @line_chart_data.values %>
        }]
    });
});
</script>
